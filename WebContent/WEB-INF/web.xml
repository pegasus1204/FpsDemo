<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  


  
  <servlet>
    <servlet-name>AuditMemberInfos</servlet-name>
    <servlet-class>com.sina.servlet.send.AuditMemberInfos</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuditMemberInfos</servlet-name>
    <url-pattern>/AuditMemberInfos</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Send</servlet-name>
    <servlet-class>com.sina.servlet.send.Send</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Send</servlet-name>
    <url-pattern>/Send</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DownLoad</servlet-name>
    <servlet-class>com.sina.servlet.send.DownLoadSftp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownLoad</servlet-name>
    <url-pattern>/DownLoad</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>Notify</servlet-name>
    <servlet-class>com.sina.tools.notify_url</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Notify</servlet-name>
    <url-pattern>/Notify</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>



